{
  "manifest_version": 2,

  "name": "AEM-er",
  "description": "AEM-er",
  "version": "1.0",

  "browser_action": {
    "default_icon": "image/aem.png",
    "default_popup": "popup.html"
  },

  "icons": {
    "16": "image/icons/16x16.png",
    "32": "image/icons/32x32.png",
    "48": "image/icons/48x48.png",
    "128": "image/icons/128x128.png"
  },

  "background": {
    "page": "background.html"
  },

  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },

  "permissions": [
    "<all_urls>",
    "cookies",
    "activeTab",
    "storage",
    "browsingData",
    "https://www.google.com/",
    "contextMenus"
  ]
}